<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dialog_clientes"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".dialogs.DialogClientes"
    android:orientation="vertical"
    android:padding="15dp"
    android:background="@color/colorWhite">

    <ImageView
        android:id="@+id/dclientes_close"
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:src="@drawable/ic_close_black_24dp"
        android:layout_alignParentRight="true"/>

    <LinearLayout
        android:id="@+id/dclientes_linear_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/dps_close"
        android:orientation="vertical"
        android:padding="15dp">

        <TextView
            android:id="@+id/dclientes_id"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:text="Id"/>

        <TextView
            android:id="@+id/dclientes_idT"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="-"/>

        <TextView
            android:id="@+id/dclientes_nombre"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="bold"

            android:text="Nombre"/>

        <TextView
            android:id="@+id/dclientes_nombreT"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="-"/>

        <TextView
            android:id="@+id/dclientes_razon"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="bold"

            android:text="Razon social"/>

        <TextView
            android:id="@+id/dclientes_razonT"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="-"/>

        <TextView
            android:id="@+id/dclientes_rfc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="bold"

            android:text="RFC"/>

        <TextView
            android:id="@+id/dclientes_rfcT"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="-"/>

        <TextView
            android:id="@+id/dclientes_estado"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="bold"

            android:text="Estado"/>

        <TextView
            android:id="@+id/dclientes_estadoT"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:text="-"/>

        <TextView
            android:id="@+id/dclientes_ciudad"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="bold"

            android:text="Ciudad"/>

        <TextView
            android:id="@+id/dclientes_ciudadT"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="-"/>

        <TextView
            android:id="@+id/dclientes_telefono"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="bold"

            android:text="TelÃ©fono"/>

        <TextView
            android:id="@+id/dclientes_telefonoT"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="-"/>

    </LinearLayout>
</RelativeLayout>